;DECLARACAO DE VARIAVEIS
DVAR $RR0
DVAR $RR1
DVAR $RR2
DVAR $RR3
DVAR $RR4
DVAR $RR5
DVAR $RR6

;CONFIGURACOES GERAIS
G54	X0 Y0 Z0 A0 B0 			;DEFINE OFFSET (ZERO PECA)
G16 A B Z 				    ;DEFINE PLANO DE TRABALHO
VELOCITY ON				    ;VELOCIDADE CONSTANTE
RAMP RATE 0				    ;MAXIMA ACELERACAO [0]
WAIT MODE AUTO

;CONFIGURACOES DO SCANER
GALVO LASERMODE A 4			;TIPO DE ACIONAMENTO DO LASER [4]
GALVO LASEROFFDELAY A 0
GALVO LASERONDELAY A 0
GALVO STANDBYPULSEWIDTH A 0.4	
GALVO LASEROVERRIDE A AUTO 

;CONFIGURACAO DO IFOV (ON-THE-FLY)
IFOV AXISPAIR 0,A,X
IFOV AXISPAIR 1,B,Y
IFOV SYNCAXES NONE
IFOV SIZE 50				;AREA DE TRABALHO DEFINIDA PARA O SCANNER
IFOV TIME 200
IFOV TRACKINGSPEED 150
IFOV TRACKINGACCEL 1000
IFOV OFF

;CONFIGURACOES PARA WOBBLE
;OPCOES DE WOBBLE: GALVOWOBBLETYPE_Ellipse - GALVOWOBBLETYPE_Figure8Parallel - GALVOWOBBLETYPE_Figure8Perpendicular
;SINTAXE: GALVO WOBBLE <GalvoAxis> CONFIG <AmplitudeParallel>, <AmplitudePerpendicular>, <Frequency>, <WobbleMode>, <WobbleType>
GALVO WOBBLE A CONFIG 1, 1, 0.5, GALVOWOBBLEMODE_DistanceBased, GALVOWOBBLETYPE_Ellipse
GALVO WOBBLE A OFF

;CONFIGURACOES PARA O LASER YLPN 50W PULSADO
GALVO STANDBYPERIOD A 20.00	;DEFINE O PERIODO EM us DA TAXA DE REPETICAO [MAX 2MHz - 0.5 us]

;ATRIBUICAO DE VALORES AS VARIAVEIS
$RR0=10					    ; COMPRIMENTO DAS LINHAS
$RR1=0.10 				    ; DISTANCIA ENTRE LINHAS
